extends layout

block content
    h1= title
    
    ul
        each roll in roll_list
            li
                a(href=roll.url) name: #{roll.name}
                | (Rating: #{roll.rating}, Adjust: #{roll.effectiveLengthAdjust}, Firm Adjust: #{roll.firm_F}, Two Adjust: #{roll.twoTimes_2})
                
        else
            li There are no rolls.